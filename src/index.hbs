<html>
  <head>
    <title>Zydis doc version index</title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700&display=swap"
      rel="stylesheet"
    />
    <script
      src="https://kit.fontawesome.com/6ca3f848c2.js"
      crossorigin="anonymous"
    ></script>
    <style>
      *, *::before, *::after { box-sizing: border-box; } html, body { height:
      100%; margin: 0; } body { background-color: #fff; color: #2f3e4e;
      min-height: 100vh; text-rendering: optimizeSpeed; line-height: 1.5;
      font-family: "Inter", sans-serif; font-size: 1rem; } h2 { font-size: 24px;
      font-weight: 700; margin-top: 0; margin-bottom: 0; } a { color: #2f3e4e;
      display: block; text-decoration: none; margin-left: 35px; padding-top:
      10px; } a:hover { color: #63c5ff; } .logo-container { display: flex;
      flex-direction: column; align-items: center; justify-content: center; }
      .wrapper { display: flex; flex-direction: column; align-items: center;
      justify-content: center; height: 100%; } .container { background-color:
      #fbfbfb; border-radius: 10px; padding: 20px; width: 300px; border: #dedede
      2px solid; } .heading-container { display: flex; flex-direction: row;
      align-items: center; gap: 8px; } .heading-container:not(:first-child) {
      margin-top: 20px; } @media (prefers-color-scheme: dark) { body {
      background-color: #1a1a1a; color: #fff; } a { color: #d4dbde; } .container
      { background-color: #252628; border-color: #38393b; } } .accordion {
      cursor: pointer; margin-left: 35px; transition: 0.4s; } .accordion:after {
      content: "\f107"; font-family: "FontAwesome"; font-weight: bold; float:
      right; margin-left: 5px; } .active:after { content: "\f106"; font-family:
      "FontAwesome"; } .panel { padding: 0 18px; max-height: 0; overflow:
      hidden; transition: max-height 0.2s ease-out; }
    </style>
  </head>

  <body>
    <div class="wrapper">
      <div class="logo-container">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          viewBox="0 0 201.51 175.77"
          aria-hidden="true"
          width="100px"
          height="100px"
        >
          <defs>
            <style>
              .cls-1, .cls-2 { fill-rule: evenodd; } .cls-1 { fill:
              url(#Neues_Verlaufsfeld_1); } .cls-2 { fill:
              url(#Neues_Verlaufsfeld_1-2); }
            </style>
            <linearGradient
              id="Neues_Verlaufsfeld_1"
              x1="72.76"
              y1="59.76"
              x2="191.1"
              y2="59.76"
              gradientTransform="matrix(0.51, 0.86, -0.86, 0.51, 124.34, -43.72)"
              gradientUnits="userSpaceOnUse"
            >
              <stop offset="0" stop-color="#6adbff"></stop>
              <stop offset="1" stop-color="#3f46ff"></stop>
            </linearGradient>
            <linearGradient
              id="Neues_Verlaufsfeld_1-2"
              x1="0.76"
              y1="116.01"
              x2="169.51"
              y2="116.01"
              xlink:href="#Neues_Verlaufsfeld_1"
            ></linearGradient>
          </defs>
          <title>Logo-Bildmarke</title>
          <g id="Ebene_2" data-name="Ebene 2">
            <g id="Logo">
              <g id="Logo-2" data-name="Logo">
                <polygon
                  id="_02"
                  data-name="02"
                  class="cls-1"
                  points="150.1 0.02 201.51 85.79 152.16 173.66 115.72 173.64 67.53 93.23 84.69 61.08 132.89 141.5 165.07 85.77 113.66 0 150.1 0.02"
                ></polygon>
                <polygon
                  id="_01"
                  data-name="01"
                  class="cls-2"
                  points="51.41 175.76 87.85 175.77 36.44 90 68.63 34.28 116.82 114.69 133.99 82.54 85.79 2.13 49.35 2.11 0 89.98 51.41 175.76"
                ></polygon>
              </g>
            </g>
          </g>
        </svg>
        <h1>Zydis documentation</h1>
      </div>
      <div class="container">
        {{#if branches}}
          <div class="heading-container">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              aria-hidden="true"
              role="img"
              width="25px"
              height="25px"
              preserveAspectRatio="xMidYMid meet"
              viewBox="0 0 16 16"
            >
              <g
                fill="none"
                stroke="#63C5FF"
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="1.3"
              >
                <circle cx="4.5" cy="3.5" r="1.75"></circle>
                <circle cx="11.5" cy="3.5" r="1.75"></circle>
                <circle cx="4.5" cy="12.5" r="1.75"></circle>
                <path d="M5.25 8.25c3 0 6 .5 6-2.5m-6.5 4.5v-4.5"></path>
              </g>
            </svg>
            <h2>Branches</h2>
          </div>
          {{#each branches}}
            <div>
              <a href="{{dir}}/html/index.html">{{short_ref}}</a>
            </div>
          {{/each}}
        {{/if}}

        {{#if tags}}
          <div class="heading-container">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              aria-hidden="true"
              role="img"
              width="25px"
              height="25px"
              preserveAspectRatio="xMidYMid meet"
              viewBox="0 0 24 24"
            >
              <g
                fill="none"
                stroke="#63C5FF"
                stroke-linecap="round"
                stroke-width="2"
              >
                <path
                  stroke-linejoin="round"
                  d="M15.244 21.366a2.164 2.164 0 0 1-3.061 0l-8.549-8.549A2.164 2.164 0 0 1 3 11.287V5.163C3 3.97 3.97 3 5.164 3h6.123c.573 0 1.124.228 1.53.634l8.549 8.549a2.164 2.164 0 0 1 0 3.061l-6.122 6.122Z"
                ></path>
                <path d="M6.5 6.5L7 7"></path>
              </g>
            </svg>
            <h2>Tags</h2>
          </div>

          {{#each tags}}
            <a class="accordion">{{major}}</a>
            <div class="panel">
              {{#each subversions}}
                <div>
                  <a href="{{dir}}/html/index.html">{{short_ref}}</a>
                </div>
              {{/each}}
            </div>
          {{/each}}
        {{/if}}

        {{#if misc_refs}}
          <div class="heading-container">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              aria-hidden="true"
              role="img"
              width="25px"
              height="25px"
              preserveAspectRatio="xMidYMid meet"
              viewBox="0 0 24 24"
            >
              <path
                fill="#63C5FF"
                d="M17.867 2.504c-.355-.624-1.381-.623-1.736 0l-3.999 7A1 1 0 0 0 13 11h8a1.001 1.001 0 0 0 .868-1.496l-4.001-7zM3 22h7a1 1 0 0 0 1-1v-7a1 1 0 0 0-1-1H3a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1zm14.5-9c-2.481 0-4.5 2.019-4.5 4.5s2.019 4.5 4.5 4.5s4.5-2.019 4.5-4.5s-2.019-4.5-4.5-4.5z"
              ></path>
            </svg>
            <h2>Misc Refs</h2>
          </div>
          {{#each misc_refs}}
            <div>
              <a href="{{dir}}/html/index.html">{{short_ref}}</a>
            </div>
          {{/each}}
        {{/if}}
      </div>
    </div>
    <script>
      var acc = document.getElementsByClassName("accordion"); var i; for (i = 0;
      i < acc.length; i++) { acc[i].addEventListener("click", function () {
      this.classList.toggle("active"); var panel = this.nextElementSibling; if
      (panel.style.maxHeight) { panel.style.maxHeight = null; } else {
      panel.style.maxHeight = panel.scrollHeight + "px"; } }); }
    </script>
  </body>
</html>